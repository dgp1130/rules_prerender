load(
    "//packages/rules_prerender:index.bzl",
    "prerender_component",
    "web_resources",
)

prerender_component(
    name = "component",
    srcs = ["component.ts"],
    visibility = ["//examples/resources:__subpackages__"],
    resources = [":resources"],
    deps = ["//examples/resources/transitive"],
)

web_resources(
    name = "resources",
    entries = {
        "/images/component.png": "image.png",
    },
)
