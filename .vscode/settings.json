{
    "typescript.tsdk": "node_modules/typescript/lib",
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "javascript.preferences.importModuleSpecifier": "project-relative",
    "typescript.preferences.importModuleSpecifier": "project-relative",
    "javascript.preferences.importModuleSpecifierEnding": "js",
    "typescript.preferences.importModuleSpecifierEnding": "js",
    "cSpell.words": [
        "bazel",
        "bazelrc",
        "Chainable",
        "chromedriver",
        "deduplicated",
        "deduplicates",
        "devserver",
        "effectful",
        "effectfully",
        "execroot",
        "expando",
        "frontmatter",
        "genfiles",
        "genrule",
        "hydroactive",
        "IFCHANGE",
        "inlines",
        "lightningcss",
        "npmjs",
        "npmrc",
        "preact",
        "prerender",
        "prerendered",
        "prerendering",
        "prerenders",
        "promisified",
        "readonly",
        "Renderable",
        "ruleset",
        "runfile",
        "runfiles",
        "semver",
        "shadowroot",
        "shadowrootmode",
        "subpackages",
        "testonly",
        "transpiled",
        "tsjs",
        "unmocked",
        "unproxied",
        "Updateable",
        "webdriverio",
        "wksp"
    ],
    "files.associations": {
        "BUILD.publish": "bazel"
    },
    "[bazel]": {
        "editor.tabSize": 4
    },
    "[css]": {
        "editor.tabSize": 4
    },
    "[starlark]": {
        "editor.tabSize": 4
    },
    "[typescript]": {
        "editor.tabSize": 4
    },
    "[typescriptreact]": {
        "editor.tabSize": 4
    },
    "[markdown]": {
        "editor.tabSize": 4
    },
    "typescript.format.semicolons": "insert",
}
